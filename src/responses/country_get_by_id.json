{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "meta": {
      "type": "object",
      "properties": {
        "href": {
          "type": "string",
          "description": "Ссылка на сущность"
        },
        "metadataHref": {
          "type": "string",
          "description": "Ссылка на метаданные сущности"
        },
        "type": {
          "type": "string",
          "description": "Тип сущности"
        },
        "mediaType": {
          "type": "string",
          "description": "Тип данных"
        }
      },
      "required": [
        "href",
        "mediaType"
      ],
      "description": "metaData"
    },
    "id": {
      "type": "string",
      "description": "ID в формате UUID"
    },
    "accountId": {
      "type": "string",
      "description": "ID учетной записи (Выводится только у созданных вручную стран)"
    },
    "version": {
      "type": "string",
      "description": "Версия сущности"
    },
    "updated": {
      "type": "string",
      "description": "Момент последнего обновления сущности"
    },
    "deleted": {
      "type": "string",
      "description": "Момент последнего удаления сущности"
    },
    "name": {
      "type": "string",
      "description": "Наименование страны"
    },
    "description": {
      "type": "string",
      "description": "Описание Страны"
    },
    "code": {
      "type": "string",
      "description": "Код сущности"
    },
    "externalCode": {
      "type": "string",
      "description": "Внешний код сущности"
    }
  },
  "additionalProperties": false
}
